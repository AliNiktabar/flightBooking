@model IEnumerable<flightbooking_project.Models.FlightBooking>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/Admin_Layout1.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TicketReservation_tbls.ResFrom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.bCusName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.bCusAddress)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.bCusEmail)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.bCusSeats)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.bCusPhoneNum)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TicketReservation_tbls.ResFrom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.bCusName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.bCusAddress)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.bCusEmail)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.bCusSeats)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.bCusPhoneNum)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.bId }) |
            @Html.ActionLink("Details", "Details", new { id=item.bId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.bId })
        </td>
    </tr>
}

</table>
